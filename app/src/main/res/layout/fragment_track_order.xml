<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".ui.home.account.orders_history.track_order.TrackOrderFragment"
    android:padding="@dimen/_10sdp"
    >

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/gilroy_semi_bold"
        android:text="Track Order"
        android:textColor="@color/title_color"
        android:textSize="@dimen/_16ssp"
        app:layout_constraintBottom_toBottomOf="@+id/track_order_back_btn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/track_order_back_btn" />


    <Button
        android:id="@+id/track_order_back_btn"
        style="@style/Widget.Material3.Button.IconButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:icon="@drawable/ic_back_arrow_round"
        app:iconTint="@color/title_color"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/track_order_id_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_14sdp"
        android:textColor="@color/title_color"
        android:fontFamily="@font/gilroy_semi_bold"
        android:textSize="@dimen/_13ssp"
        android:text="Order#168"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/track_order_back_btn" />

    <TextView
        android:id="@+id/track_order_date_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/gilroy_medium"
        android:layout_marginTop="@dimen/_2sdp"
        android:text="Mon, 23 Aug 2023"
        android:textColor="@color/subtitle_color"
        android:textSize="@dimen/_11ssp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/track_order_id_tv" />

    <ImageView
        android:id="@+id/track_order_placed_imv"
        android:layout_width="0dp"
        android:layout_height="@dimen/_20sdp"
        android:layout_marginTop="28dp"
        android:src="@drawable/ic_unchecked_radio_button"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.12"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/track_order_date_tv" />

    <TextView
        android:id="@+id/textView27"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/gilroy_semi_bold"
        android:text="Order Placed"
        android:textColor="@color/title_color"
        android:textSize="@dimen/_14ssp"
        app:layout_constraintBottom_toBottomOf="@+id/track_order_placed_imv"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.1"
        app:layout_constraintStart_toEndOf="@+id/track_order_placed_imv"
        app:layout_constraintTop_toTopOf="@+id/track_order_placed_imv" />

    <TextView
        android:id="@+id/textView29"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/gilroy_semi_bold"
        android:text="Processing"
        android:textColor="@color/title_color"
        android:textSize="@dimen/_14ssp"
        app:layout_constraintBottom_toBottomOf="@+id/track_order_processing_imv"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.1"
        app:layout_constraintStart_toEndOf="@+id/track_order_processing_imv"
        app:layout_constraintTop_toTopOf="@+id/track_order_processing_imv" />

    <TextView
        android:id="@+id/textView31"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/gilroy_semi_bold"
        android:text="Shipped"
        android:textColor="@color/title_color"
        android:textSize="@dimen/_14ssp"
        app:layout_constraintBottom_toBottomOf="@+id/track_order_shipped_imv"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.1"
        app:layout_constraintStart_toEndOf="@+id/track_order_shipped_imv"
        app:layout_constraintTop_toTopOf="@+id/track_order_shipped_imv" />

    <TextView
        android:id="@+id/textView33"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/gilroy_semi_bold"
        android:text="Delivered"
        android:textColor="@color/title_color"
        android:textSize="@dimen/_14ssp"
        app:layout_constraintBottom_toBottomOf="@+id/track_order_delivered_imv"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.1"
        app:layout_constraintStart_toEndOf="@+id/track_order_shipped_imv"
        app:layout_constraintTop_toTopOf="@+id/track_order_delivered_imv" />

    <View
        android:id="@+id/view3"
        android:layout_width="2dp"
        android:layout_height="@dimen/_50sdp"
        android:layout_marginTop="@dimen/_6sdp"
        android:background="@color/subtitle_color"
        app:layout_constraintEnd_toEndOf="@+id/track_order_placed_imv"
        app:layout_constraintStart_toStartOf="@+id/track_order_placed_imv"
        app:layout_constraintTop_toBottomOf="@+id/track_order_placed_imv" />

    <View
        android:id="@+id/view4"
        android:layout_width="2dp"
        android:layout_height="@dimen/_50sdp"
        android:layout_marginTop="@dimen/_6sdp"
        android:background="@color/subtitle_color"
        app:layout_constraintEnd_toEndOf="@+id/track_order_placed_imv"
        app:layout_constraintStart_toStartOf="@+id/track_order_placed_imv"
        app:layout_constraintTop_toBottomOf="@+id/track_order_processing_imv" />

    <View
        android:id="@+id/view7"
        android:layout_width="2dp"
        android:layout_height="@dimen/_50sdp"
        android:layout_marginTop="@dimen/_6sdp"
        android:background="@color/subtitle_color"
        app:layout_constraintEnd_toEndOf="@+id/track_order_placed_imv"
        app:layout_constraintStart_toStartOf="@+id/track_order_placed_imv"
        app:layout_constraintTop_toBottomOf="@+id/track_order_shipped_imv" />

    <ImageView
        android:id="@+id/track_order_processing_imv"
        android:layout_width="0dp"
        android:layout_height="@dimen/_20sdp"
        android:layout_marginTop="@dimen/_6sdp"
        android:src="@drawable/ic_unchecked_radio_button"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintEnd_toEndOf="@+id/track_order_placed_imv"
        app:layout_constraintStart_toStartOf="@+id/track_order_placed_imv"
        app:layout_constraintTop_toBottomOf="@+id/view3" />

    <ImageView
        android:id="@+id/track_order_shipped_imv"
        android:layout_width="0dp"
        android:layout_height="@dimen/_20sdp"
        android:layout_marginTop="@dimen/_6sdp"
        android:src="@drawable/ic_unchecked_radio_button"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintEnd_toEndOf="@+id/track_order_placed_imv"
        app:layout_constraintStart_toStartOf="@+id/track_order_placed_imv"
        app:layout_constraintTop_toBottomOf="@+id/view4" />

    <ImageView
        android:id="@+id/track_order_delivered_imv"
        android:layout_width="0dp"
        android:layout_height="@dimen/_20sdp"
        android:src="@drawable/ic_unchecked_radio_button"
        android:layout_marginTop="@dimen/_6sdp"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintEnd_toEndOf="@+id/track_order_placed_imv"
        app:layout_constraintStart_toStartOf="@+id/track_order_placed_imv"
        app:layout_constraintTop_toBottomOf="@+id/view7" />

    <TextView
        android:id="@+id/textView25"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:padding="@dimen/_8sdp"
        android:fontFamily="@font/gilroy_medium"
        android:textColor="@color/subtitle_color"
        android:textSize="@dimen/_12ssp"
        android:text="We have received your order and it is being processed. Thank you for your purchase!"
        app:layout_constraintBottom_toTopOf="@+id/track_order_processing_imv"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/textView27"
        app:layout_constraintTop_toBottomOf="@+id/textView27" />

    <TextView
        android:id="@+id/textView30"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/gilroy_medium"
        android:padding="@dimen/_8sdp"
        android:text="Your order is being prepared for shipment. Please be patient until the processing finish"
        android:textColor="@color/subtitle_color"
        android:textSize="@dimen/_12ssp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/textView27"
        app:layout_constraintTop_toBottomOf="@+id/textView29" />

    <TextView
        android:id="@+id/textView32"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/gilroy_medium"
        android:padding="@dimen/_8sdp"
        android:text="Your order is being prepared for shipment. Please be patient until the processing finish"
        android:textColor="@color/subtitle_color"
        android:textSize="@dimen/_12ssp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/textView27"
        app:layout_constraintTop_toBottomOf="@+id/textView31" />

    <TextView
        android:id="@+id/textView34"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/gilroy_medium"
        android:padding="@dimen/_8sdp"
        android:text="Your order has been delivered to the address you provided."
        android:textColor="@color/subtitle_color"
        android:textSize="@dimen/_12ssp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/textView27"
        app:layout_constraintTop_toBottomOf="@+id/track_order_delivered_imv" />

</androidx.constraintlayout.widget.ConstraintLayout>